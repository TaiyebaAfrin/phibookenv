{
  "version": 2,
  "builds": [
    {
      "src": "**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "phibook/wsgi.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9",
    "DJANGO_SETTINGS_MODULE": "phibook.settings"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }
}
